int csd_main()
{
	unsigned int * sw;
	sw = (unsigned *)0x41210000;

	long long epoch = 0;

	if ((*sw) == 0x0)
		epoch = 50000000000;		// 1 second (패치 시간 포함)
	else if ((*sw) == 0x1)
		epoch = 40000000000;
	else if ((*sw) == 0x2)
		epoch = 35000000000;
	else if ((*sw) == 0x4)
		epoch = 30000000000;
	else if ((*sw) == 0x8)
		epoch = 25000000000;
	else if ((*sw) == 0x10)
		epoch = 20000000000;
	else if ((*sw) == 0x20)
		epoch = 15000000000;
	else if ((*sw) == 0x40)
		epoch = 10000000000;
	else if ((*sw) == 0x80)
		epoch = 5000000000;

	for (int i = 0; i < epoch;)
		i++;

	return 0;
}
