int csd_main(int a)
{
	unsigned int * uart_TX_RX_FIFO0;
	uart_TX_RX_FIFO0 = (unsigned *)0xE001030;

	unsigned int * uart_Channel_sts_reg0;
	uart_Channel_sts_reg0 = (unsigned *)0xE00102C;


	long long epoch = 0;

	if ((*uart_TX_RX_FIFO0) >= 0x80)
		epoch = 0x888888;		// roughly 100 msec
	else if ((*uart_TX_RX_FIFO0) >= 0x40)
		epoch = 0x1111110;
	else if ((*uart_TX_RX_FIFO0) >= 0x20)
		epoch = 0x1999998;
	else if ((*uart_TX_RX_FIFO0) >= 0x10)
		epoch = 0x2222220;
	else if ((*uart_TX_RX_FIFO0) >= 0x8)
		epoch = 0x2AAAAA8;
	else if ((*uart_TX_RX_FIFO0) >= 0x4)
		epoch = 0x3333330;
	else if ((*uart_TX_RX_FIFO0) >= 0x2)
		epoch = 0x3BBBBB8;
	else if ((*uart_TX_RX_FIFO0) >= 0x1)
		epoch = 0x4444440;
	else if ((*uart_TX_RX_FIFO0) >= 0x0)
		epoch = 0x5555550;

	for (int i = 0; i < epoch; i++);

	return 0;
}
