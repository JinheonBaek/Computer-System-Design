int csd_main()
{
	unsigned int * sw;
	sw = (unsigned *)0x41210000;

	long long epoch = 0;

	if ((*sw) == 0x0)
		epoch = 0x10000000;		// 1 second (패치 시간 포함)
	else if ((*sw) == 0x1)
		epoch = 0xCCCCCC8;
	else if ((*sw) == 0x2)
		epoch = 0xB33332F;
	else if ((*sw) == 0x4)
		epoch = 0x9999996;
	else if ((*sw) == 0x8)
		epoch = 0x7FFFFFD;
	else if ((*sw) == 0x10)
		epoch = 0x6666664;
	else if ((*sw) == 0x20)
		epoch = 0x4CCCCCB;
	else if ((*sw) == 0x40)
		epoch = 0x3333332;
	else if ((*sw) == 0x80)
		epoch = 0x1999999;

	for (int i = 0; i < epoch; i++);

	return 0;
}
